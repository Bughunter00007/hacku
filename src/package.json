  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreamnettech/dreamtime.git",
    "directory": "src"
  },
  "bugs": {
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/dreamtime/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "lint-staged": {
    "*.{js,vue}": "eslint --fix --ignore-path .gitignore"
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "v",
    "releaseCommitMessageFormat": "chore(release): {{currentTag}}",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "improvement",
        "section": "Improvements"
      },
      {
        "type": "docs",
        "section": "Miscellaneous changes"
      },
      {
        "type": "style",
        "section": "Design"
      },
      {
        "type": "refactor",
        "section": "Miscellaneous changes"
      },
      {
        "type": "perf",
        "section": "Improvements"
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "chore",
        "section": "Miscellaneous changes"
      }
    ]
  },
