{
  "name": "dreamtime",
  "version": "1.6.3",
  "displayName": "DreamTime",
  "description": "The easiest to use application to create fake nudes from photos and videos.",
  "private": true,
  "homepage": "https://www.dreamtime.tech",
  "main": "electron/dist/index.js",
  "license": "GPL-3.0",
  "scripts": {
    "postinstall": "patch-package",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "env-cmd -e default,test mocha",
    "clean": "shx rm -rf .env .nuxt dist electron/dist",
    "deploy": "node scripts/deploy.js",
    "develop": "env-cmd -e default,development --no-override",
    "production": "env-cmd -e default,production --no-override",
    "optimize": "modclean --patterns default:safe --run",
    "dev:env": "yarn develop node scripts/env.js",
    "dev:nuxt": "yarn develop nuxt",
    "dev:babel": "yarn develop babel electron/src -d electron/dist -s -w --verbose",
    "dev:electron": "yarn develop electron .",
    "dev:build": "yarn run clean && yarn build:nuxt && yarn build:babel && yarn run dev:env && yarn build:electron",
    "build:env": "yarn production node scripts/env.js",
    "build:nuxt": "yarn production nuxt generate",
    "build:babel": "yarn production babel electron/src -d electron/dist --minified",
    "build:electron": "yarn production electron-builder --publish=never --x64",
    "build": "yarn run clean && yarn build:nuxt && yarn build:babel && yarn run build:env && yarn build:electron",
    "preview:electron": "env-cmd -e default,production,preview --no-override electron .",
    "preview:production": "yarn build:nuxt && yarn build:babel && yarn preview:electron"
  },
  "author": {
    "name": "Ivan Bravo Bravo",
    "email": "kolessios@gmail.com",
